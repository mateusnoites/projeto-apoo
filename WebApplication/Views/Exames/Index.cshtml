@model IEnumerable<WebApplication.Models.Exame>

@{Layout=null;}
    


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p id="create">
        @Html.ActionLink("Criar Exame", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descricao)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Descricao)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.ExameId }) |
                    @Html.ActionLink("Details", "Details", new { id = item.ExameId }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.ExameId })
                </td>
            </tr>
        }

    </table>
</body>
</html>
